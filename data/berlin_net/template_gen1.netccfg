<?xml version="1.0" encoding="UTF-8"?>

<configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://sumo.dlr.de/xsd/netconvertConfiguration.xsd">

    <input>
        <node-files value="netpatch/patch.nod.xml"/>
        <edge-files value="netpatch/patch.edg.xml, netpatch/patch_tram.edg.xml"/>
        <connection-files value="netpatch/patch.con.xml, netpatch/patch_tram.con.xml"/>
        <!-- disabled due to new joined tls logics 2022-12-20
        <tllogic-files value="netpatch/patch.tll.xml"/>
        -->
    </input>

    <output>
        <output.street-names value="true"/>
        <output.original-names value="true"/>
        <proj.plain-geo value="true"/>
    </output>

    <tls_building>
        <tls.default-type value="actuated"/>
    </tls_building>

    <report>
        <verbose value="true"/>
        <log value="netconvert.log"/>
    </report>

</configuration>
